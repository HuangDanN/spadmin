<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yirong.framework.mapper.OperLogsMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.yirong.framework.entity.OperLogs">
		<id column="ID" property="id" />
		<result column="USER_NAME" property="userName" />
		<result column="OPEA_TIME" property="opeaTime" />
		<result column="CLAZZ" property="clazz" />
		<result column="METHOD" property="method" />
		<result column="IP" property="ip" />
		<result column="CONTENT" property="content" />
		<result column="MODEL_CODE" property="modelCode" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID AS id, USER_NAME AS userName, OPEA_TIME AS opeaTime, CLAZZ AS clazz, METHOD AS method, IP AS ip, CONTENT AS content, MODEL_CODE AS modelCode
    </sql>

</mapper>
