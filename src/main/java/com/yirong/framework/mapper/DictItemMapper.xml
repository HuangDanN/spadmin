<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yirong.framework.mapper.DictItemMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.yirong.framework.entity.DictItem">
		<id column="ITEM_ID" property="itemId" />
		<result column="DICT_ID" property="dictId" />
		<result column="ITEM_NAME" property="itemName" />
		<result column="ITEM_CODE" property="itemCode" />
		<result column="ITEM_VALUE" property="itemValue" />
		<result column="IS_ENABLE" property="isEnable" />
		<result column="CREATE_DATE" property="createDate" />
		<result column="UPDATE_DATE" property="updateDate" />
		<result column="NOTE" property="note" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ITEM_ID AS itemId, DICT_ID AS dictId, ITEM_NAME AS itemName, ITEM_CODE AS itemCode, ITEM_VALUE AS itemValue, IS_ENABLE AS isEnable, CREATE_DATE AS createDate, UPDATE_DATE AS updateDate, NOTE AS note
    </sql>

</mapper>
