<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yirong.framework.mapper.DictMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.yirong.framework.entity.Dict">
		<id column="DICT_ID" property="dictId" />
		<result column="DICT_NAME" property="dictName" />
		<result column="DICT_CODE" property="dictCode" />
		<result column="DICT_TYPE" property="dictType" />
		<result column="IS_ENABLE" property="isEnable" />
		<result column="CREATE_DATE" property="createDate" />
		<result column="UPDATE_DATE" property="updateDate" />
		<result column="NOTE" property="note" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DICT_ID AS dictId, DICT_NAME AS dictName, DICT_CODE AS dictCode, DICT_TYPE AS dictType, IS_ENABLE AS isEnable, CREATE_DATE AS createDate, UPDATE_DATE AS updateDate, NOTE AS note
    </sql>

</mapper>
